{
  "name": "educours-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Backend API for EduCours Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "prod": "NODE_ENV=production node src/server.js",
    "prod:cluster": "NODE_ENV=production node src/cluster.js",
    "dev-mock": "MOCK_PORT=5002 node server-no-db.js",
    "mock": "node server-no-db.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:integration": "jest integration/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js",
    "health": "node scripts/health-check.js",
    "backup": "node scripts/backup-db.js",
    "clean": "node scripts/clean-temp.js",
    "audit": "npm audit --production",
    "docker:build": "docker build -t educours-backend ."
  },
  "dependencies": {
    "agora-access-token": "^2.0.1",
    "axios": "^1.3.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.35.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-session": "^1.17.3",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^6.15.0",
    "fs-extra": "^11.3.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.3",
    "moment": "^2.29.4",
    "mongoose": "^7.8.8",
    "mongoose-autopopulate": "^1.0.0",
    "bcrypt": "^5.1.1",
    "joi": "^17.9.2",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "winston-daily-rotate-file": "^4.7.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.13.0",
    "socket.io": "^4.6.1",
    "speakeasy": "^2.0.0",
    "stripe": "^12.0.0",
    "twilio": "^4.9.0",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.4",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
